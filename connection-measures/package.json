{
  "name": "@nzz/et-utils-connection-measures",
  "version": "0.0.1",
  "description": "Reads connection infos and/or measures the speed for a high/low resolution decision",
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nzzdev/ed-tech-utilities.git",
    "directory": "connection-measures"
  },
  "scripts": {
    "prepublishOnly": "npm run test && npm run clean && npm run buildMjs && npm run buildCjs && ./scripts/package-fixup.sh",
    "clean": "rimraf dist",
    "buildMjs": "tsc -p tsconfig.json",
    "buildCjs": "tsc -p tsconfig-cjs.json",
    "start": "npm run buildMjs -- -w",
    "test": "jest --config jest.config.ts"
  },
  "author": "Andrea Br√ºhlmann",
  "license": "MIT",
  "dependencies": {
    "@cloudflare/speedtest": "^1.6.0"
  },
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "@types/node": "^16.11.36",
    "jest": "^28.1.0",
    "rimraf": "^6.0.1",
    "svelte": "^5.0.0",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.8.0",
    "typescript": "^5.1.3"
  }
}
